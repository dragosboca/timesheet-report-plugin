{
  "name": "timesheet-report",
  "version": "1.0.0",
  "description": "This is a timesheet report plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "npm run build:parser && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:parser": "node scripts/build-parser.js",
    "install:plugin": "node scripts/install-plugin.js",
    "dev:install": "npm run build && npm run install:plugin",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "timesheet",
    "reporting",
    "query-language",
    "parser"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.25.0",
    "jest": "^29.0.0",
    "obsidian": "latest",
    "peggy": "^5.0.6",
    "ts-jest": "^29.0.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "chart.js": "^3.9.1"
  }
}
